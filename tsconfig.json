{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "typeRoots": [
      "./@types",
      "./node_modules/@types"
    ],
    "types": [
      "@expo/vector-icons",
      "node"
    ]
  },
  "extends": "expo/tsconfig.base.json"
}
